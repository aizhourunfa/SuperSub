{
  "name": "supersub",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel start:frontend start:backend",
    "start:backend": "wrangler pages dev ./dist --local --persist-to ./.wrangler/state --port 8792",
    "start:frontend": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "db:init": "wrangler d1 execute supersub-db --file=./db/schema.sql --local"
  },
  "dependencies": {
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.4",
    "@types/js-yaml": "^4.0.9",
    "@types/uuid": "^10.0.0",
    "@vicons/ionicons5": "^0.13.0",
    "axios": "^1.12.2",
    "bcrypt-ts": "^7.1.0",
    "date-fns": "^4.1.0",
    "hono": "^4.9.8",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.43.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "uuid": "^13.0.0",
    "vfonts": "^0.0.3",
    "vue": "^3.3.4",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250926.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.19.127",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.1.6",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^4.4.8",
    "vitest": "^3.2.4",
    "vue-tsc": "^1.8.8",
    "wait-on": "^9.0.1",
    "wrangler": "^4.40.1"
  }
}
